page{
  background: white;
  height: 100%;
  width:100%;
  position: relative;
  overflow-x:hidden;
}
#cover{
  width:100%;
  height:100%;
  background: rgba(0, 0, 0, 0.7);
  position: absolute;
  top:0;
  z-index: 99;
  display: flex;
  align-items: center;
}
#startTxt{
  width:100%;
  height:220rpx;
  color:#EEC23C;
  line-height: 220rpx;
  font-size: 200rpx;
  text-align: center;
}
#bg{
  background: #EEC23C;
  width:220%;
  height:75%;
  border-radius: 0 0 50% 50%;
  position: absolute; left: -60%; top: 0;
  display: flex;
  flex-direction: column;
  z-index:1;
  align-items: center;
  overflow-x: hidden;
}
#list{
 
  width:100%;
  height:80rpx;
  margin-top: 20rpx;
  line-height: 80rpx;
  z-index:2;
  color:white;
  text-align: center;
  font-size: 55rpx;
}
.itemTitle{
  width:220%;
  height:60rpx;
  margin-top: 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
}
.itemTitleName{

  width:150rpx;
  height:60rpx;
  line-height: 60rpx;
  z-index:2;
  color:white;
  text-align: center;
  font-size: 36rpx;
}
.item{
  width:220%;
  height:80rpx;
  margin-top: 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  position: relative;
}
.medal{
  width:80rpx;
  height:80rpx;
}
.index{

  color:white;
  width:80rpx;
  height:80rpx;
  line-height: 80rpx;
  position:absolute;
  left:1475rpx;
  top:-10rpx;
  z-index:99;
  font-size: 40rpx;
  text-align: center;
  font-weight: bold;
}
.itemTxt{
  width:150rpx;
  height:80rpx;
  line-height: 80rpx;
  z-index:2;
  color:white;
  text-align: center;
  font-size: 40rpx;
}
#title{
  position: absolute;
  top:80%;
  text-align: center;
  width:100%;
  height:80rpx;
  line-height: 80rpx;
  font-size: 55rpx;
}
.detail{
 
  text-align: center;
  width:100%;
  height:80rpx;
  line-height: 80rpx;
  font-size: 25rpx;
  color:#E83C26;
}
#d0{
  position: absolute;
  top:88%;
}
#d1{
  position: absolute;
  top:91%;
}
.btn{
  width:320rpx;
  height:100rpx;
  color:white;
  line-height: 100rpx;
  text-align: center;
  margin-top: 75rpx;
  border-radius: 50rpx;
}
#look{

  background: #8C771E;
 
}
#go{
  background: #E83C26;
}